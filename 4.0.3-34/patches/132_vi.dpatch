+msgstr "Minimo:\t%ld\n"
+
+#: src/chage.c:251
+#, c-format
+msgid "Maximum:\t%ld\n"
+msgstr "Massimo:\t%ld\n"
+
+#: src/chage.c:253
+#, c-format
+msgid "Warning:\t%ld\n"
+msgstr "Avviso:\t%ld\n"

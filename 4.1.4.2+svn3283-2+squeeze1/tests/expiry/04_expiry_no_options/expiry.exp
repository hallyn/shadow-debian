#!/usr/bin/expect

set timeout 2
expect_after default {puts "\nFAIL"; exit 1}

spawn /bin/sh
send "if \[ \$(id -u) -eq 0 \]; then PS1='# '; else PS1='$ '; fi\r"
expect "# "

send "expiry\r"
expect "Usage: expiry {-f|-c}"
expect "# "
send "echo \$?\r"
expect "10"
expect "# "
send "exit\r"
puts "OK\n"
exit 0

+#: ../../login.defs.5:155
+msgid ""
+"CREATE_HOME GID_MAX GID_MIN PASS_MAX_DAYS PASS_MIN_DAYS PASS_WARN_AGE "
+"UID_MAX UID_MIN UMASK"
+msgstr ""
+
+# type: IP
+#: ../../login.defs.5:155
+#, no-wrap
+msgid "userdel"
+msgstr ""
+
+# type: Plain text
+#: ../../login.defs.5:158
+msgid "MAIL_DIR USERDEL_CMD"

+#: ../../useradd.8:46
+msgid "[B<-e>I< default_expire_date>] [B<-f>I< default_inactive>]"
+msgstr "[B<-e>I< data_scadenza_predefinita>] [B<-f>I< inattivitÃ _predefinita>]"
